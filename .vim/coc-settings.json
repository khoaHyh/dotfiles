{
	"diagnostic.displayByAle": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "css",
    "rust",
    "go"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  }
}
